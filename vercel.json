{
    "version": 2,
    "name": "petgram-jboss",
    "builds": [
        { "use": "@vercel/static-build", "src": "package.json"}
    ],
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/$1.js"            
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"            
        },
        {
            "src": "/.*",
            "dest": "index.html"
        }
    ]
}